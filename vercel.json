{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "index.ts"
    },
    {
      "src": "/index.tsx",
      "dest": "index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "index.ts"
    }
  ]
}